package ArrayList;

public class Square {
	
	private double side;
	
	public Square() {
		
	}
	
	public Square(double side) {
		this.side = side;
	}//end empty-argument constructor
	
	public double getArea() {
		return side * side;		
	}//end getArea

	@Override
	public String toString() {
		return "Square [side=" + side + "]";
	}//end toString

}//end class
